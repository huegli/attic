		<DisplayString Condition="mpEnd&gt;=mpBegin">len={mpEnd-mpBegin,i} "{mpBegin,[mpEnd-mpBegin]sub}"</DisplayString>
		<DisplayString Condition="mpEnd&lt;mpBegin">invalid!</DisplayString>
		<StringView>mpBegin,su</StringView>

