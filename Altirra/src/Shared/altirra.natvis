  <Type Name="ATVIA6522Emulator" ExcludeView="default">
    <Expand>
      <Item Name="Port A">*this,view(porta)</Item>
      <Item Name="Port B">*this,view(portb)</Item>
      <Item Name="CA1 mode" Condition="mPCR &amp; 1">"Positive edge",sb</Item>
      <Item Name="CA1 mode" Condition="!(mPCR &amp; 1)">"Negative edge",sb</Item>
      <Item Name="CB1 mode" Condition="mPCR &amp; 16">"Positive edge",sb</Item>
      <Item Name="CB1 mode" Condition="!(mPCR &amp; 16)">"Negative edge",sb</Item>
      <Item Name="CA2 mode" Condition="(mPCR &amp; 14)==0">"Input mode (negative edge)",sb</Item>
      <Item Name="CA2 mode" Condition="(mPCR &amp; 14)==2">"Independent interrupt input mode (negative edge)",sb</Item>
      <Item Name="CA2 mode" Condition="(mPCR &amp; 14)==4">"Input mode (positive edge)",sb</Item>
      <Item Name="CA2 mode" Condition="(mPCR &amp; 14)==6">"Independent interrupt input mode (positive edge)",sb</Item>
      <Item Name="CA2 mode" Condition="(mPCR &amp; 14)==8">"Handshake output mode (CA2 low on access and high on CA1 transition)",sb</Item>
      <Item Name="CA2 mode" Condition="(mPCR &amp; 14)==10">"Pulse output mode (CA2 low on ORA access)",sb</Item>
      <Item Name="CA2 mode" Condition="(mPCR &amp; 14)==12">"Manual output low",sb</Item>
      <Item Name="CA2 mode" Condition="(mPCR &amp; 14)==14">"Manual output high",sb</Item>
      
      <Item Name="CB2 mode" Condition="(mPCR &amp; 224)==0">"Input mode (negative edge)",sb</Item>
      <Item Name="CB2 mode" Condition="(mPCR &amp; 224)==32">"Independent interrupt input mode (negative edge)",sb</Item>
      <Item Name="CB2 mode" Condition="(mPCR &amp; 224)==64">"Input mode (positive edge)",sb</Item>
      <Item Name="CB2 mode" Condition="(mPCR &amp; 224)==96">"Independent interrupt input mode (positive edge)",sb</Item>
      <Item Name="CB2 mode" Condition="(mPCR &amp; 224)==128">"Handshake output mode (CB2 low on access and high on CB1 transition)",sb</Item>
      <Item Name="CB2 mode" Condition="(mPCR &amp; 224)==160">"Pulse output mode (CB2 low on ORB access)",sb</Item>
      <Item Name="CB2 mode" Condition="(mPCR &amp; 224)==192">"Manual output low",sb</Item>
      <Item Name="CB2 mode" Condition="(mPCR &amp; 224)==224">"Manual output high",sb</Item>
      
      <Item Name="PA Latch">(mACR &amp; 1) != 0</Item>
      <Item Name="PB Latch">(mACR &amp; 2) != 0</Item>
      
      <Item Name="T1 Control" Condition="(mACR &amp; 192)==0">"One-shot mode",sb</Item>
      <Item Name="T1 Control" Condition="(mACR &amp; 192)==64">"Free-running mode",sb</Item>
      <Item Name="T1 Control" Condition="(mACR &amp; 192)==128">"One-shot mode + PB7 output",sb</Item>
      <Item Name="T1 Control" Condition="(mACR &amp; 192)==192">"Free-running mode + PB7 output",sb</Item>
      
      <Item Name="T2 Control" Condition="!(mACR &amp; 32)">"One-shot interval timer",sb</Item>
      <Item Name="T2 Control" Condition="mACR &amp; 32">"PB6 pulse counter",sb</Item>
      
      <Item Name="Shift Control" Condition="(mACR &amp; 28)==0">"Disabled",sb</Item>
      <Item Name="Shift Control" Condition="(mACR &amp; 28)==4">"Shift in T2",sb</Item>
      <Item Name="Shift Control" Condition="(mACR &amp; 28)==8">"Shift in system clock",sb</Item>
      <Item Name="Shift Control" Condition="(mACR &amp; 28)==12">"Shift in external clock",sb</Item>
      <Item Name="Shift Control" Condition="(mACR &amp; 28)==16">"Free-running T2",sb</Item>
      <Item Name="Shift Control" Condition="(mACR &amp; 28)==20">"Shift out T2",sb</Item>
      <Item Name="Shift Control" Condition="(mACR &amp; 28)==24">"Shift out system clock",sb</Item>
      <Item Name="Shift Control" Condition="(mACR &amp; 28)==28">"Shift out external clock",sb</Item>
      
      <Item Name="Interrupt flags">*this,view(ifr)</Item>
      <Item Name="Interrupt enables">*this,view(ier)</Item>
    </Expand>
  </Type>
  
