#==========================================================================
# cached-image.cgp
#
# This is an example of a pass caching. Pass 0 does not depend on the input
# texture, so it is evaluated once and then cached.
#

shaders = "2"

shader0 = "cached-image-generate.fx"
scale_type0 = "absolute"
scale0 = "128"

shader1 = "cached-image-draw.fx"
scale_type1 = "viewport"
scale1 = "1"
